{
  "name": "open-tv",
  "version": "1.0.0",
  "scripts": {
    "androidtv": "npx expo run:android",
    "tvos": "npx expo run:ios",
    "clean:androidtv": "cd android && ./gradlew clean && cd ..",
    "clean:tvos": "cd ios && pod cache clean --all && rm -rf build && rm -rf Pods && cd ..",
    "prettier": "prettier ./src --write",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "expo": "~51.0.31",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-video": "^1.1.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@~0.74.5-0",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.3.0",
    "rn-secure-storage": "^3.0.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-native-tvos/config-tv": "^0.0.10",
    "@types/react": "~18.2.45",
    "prettier": "3.2.5",
    "typescript": "~5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true
}
