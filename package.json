{
  "name": "monaco-tv",
  "version": "1.0.0",
  "dependencies": {
    "@react-navigation/native-stack": "^6.9.26",
    "expo": "^50.0.14",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "npm:react-native-tvos@^0.73.6-3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@babel/core": "~7.20.0",
    "@react-native-tvos/config-tv": "^0.0.9",
    "@types/react": "~18.2.45",
    "typescript": "^5.3.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "private": true,
  "scripts": {
    "androidtv": "npx expo run:android",
    "tvos": "npx expo run:ios",
    "clean:androidtv": "cd android && ./gradlew clean && cd ..",
    "clean:tvos": "cd ios && pod cache clean --all && rm -rf build && rm -rf Pods && cd .."
  }
}
